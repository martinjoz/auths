extends base 

block content 
 .content-page
  .content
    // Start Content
    .container-fluid
      .row.mt-2
        .col-xl-3.col-md-6
          .card-box
            h4.header-title.mt-0.mb-4 Purchased Domains
            .widget-chart-1
              .widget-detail-1.text-right
                h2.font-weight-normal 0
                p.text-muted.mb-1 Active
        .col-xl-9.col-md-6
          .card-box
            .table-responsive(style='height: 126px')
              table.table.table-striped.mb-0
                h4.header-title Your Domains
                thead
                  tr
                    th #
                    th Domain Url
                    th Package
                    th Purchased
                    th Action
                tbody
                  tr
                    th(scope='row') 1
                    td techkey.co.ke
                    td Basic
                    td 24/1/22
                    td
                      a.btn.btn-purple.w-md.waves-effect.waves-light(href='/domaindetails')
                        | View
                  tr
                    th(scope='row') 2
                    td techgen.com
                    td Enterprise
                    td 12/8/4
                    td
                      a.btn.btn-purple.w-md.waves-effect.waves-light(href='/domaindetails')
                        | View
        .col-xl-12.col-md-12
          .card
            .card-body.p-4
              h3.header-title(style='font-size: 26px')
                | Search for a prefered domain name
              form(action='')
                .form-group.mb-2
                  label(for='domainname')
                    | If a domain is free and available, you will be able to
                    | proceed with your purchase
                  h4.m-2.hidden(style='\
                  font-weight: 300;\
                  color: red;\
                  text-align: center;\
                  ')
                    svg.bi.bi-exclamation-circle-fill(xmlns='http://www.w3.org/2000/svg' width='22' height='22' fill='currentColor' viewbox='0 0 16 16')
                      path(d='M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8 4a.905.905 0 0 0-.9.995l.35 3.507a.552.552 0 0 0 1.1 0l.35-3.507A.905.905 0 0 0 8 4zm.002 6a1 1 0 1 0 0 2 1 1 0 0 0 0-2z')
                    |                           Domain name techkey.co.ke is already taken, please try
                    |                           another
                  h4.m-2.hidden(style='\
                  font-weight: 300;\
                  color: rgb(17, 0, 255);\
                  text-align: center;\
                  ')
                    svg.bi.bi-info-lg(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewbox='0 0 16 16')
                      path(d='m10.277 5.433-4.031.505-.145.67.794.145c.516.123.619.309.505.824L6.101 13.68c-.34 1.578.186 2.32 1.423 2.32.959 0 2.072-.443 2.577-1.052l.155-.732c-.35.31-.866.434-1.206.434-.485 0-.66-.34-.536-.939l1.763-8.278zm.122-3.673a1.76 1.76 0 1 1-3.52 0 1.76 1.76 0 0 1 3.52 0z')
                    |                           techkey.co.ke is available for purchase
                  h4.hidden(style='font-weight: 300; text-align: center')
                    .spinner-border.text-purple(role='status')
                    |                           Searching for techkey.co.ke..
                  input#domainname.form-control(type='text' required='' placeholder='example.com')
                .form-group.mb-0.text-center
                  button.btn.btn-primary.btn-block(style='background-color: #1a2ba7de' type='submit')
                    | Check Avalability
      .row
        .col-lg-12
          .row
            // Pricing Column
            article.pricing-column.col-xl-4.col-md-6
              .ribbon
                span POPULAR
              .inner-box.card-box
                .plan-header.p-3.text-center
                  h3.plan-title Basic
                  h2.plan-price.font-weight-normal Ksh 4,800
                  .plan-duration Per Year
                ul.plan-stats.list-unstyled.text-center.p-3.mb-0
                  li 10GB Disk space
                  li unlimited Bandwidth
                  li unlimited Databases
                  li unlimited Emails
                  li unlimited Subdomains
                  li Softaculous Apps
                  li SEO tools
                  li email support
                  li 2 years free SSL
                br
                br
                .text-center
                  a.btn.btn-primary.waves-effect.waves-light(style='background-color: #0043ffde' href='#')
                    | purchase Basic
            // Pricing Column
            article.pricing-column.col-xl-4.col-md-6
              .inner-box.card-box
                .plan-header.p-3.text-center
                  h3.plan-title Enterprise
                  h2.plan-price.font-weight-normal ksh 7,800
                  .plan-duration Per Year
                ul.plan-stats.list-unstyled.text-center.p-3.mb-0
                  li 20GB Disk space
                  li unlimited Bandwidth
                  li unlimited Databases
                  li unlimited Emails
                  li unlimited Subdomains
                  li Softaculous Apps
                  li SEO tools
                  li email and phone support
                  li Visitors metrics
                  li 3 years free SSL
                .text-center
                  a.btn.btn-primary.waves-effect.waves-light(style='background-color: #0043ffde' href='#')
                    | purchase Enterprise
            // Pricing Column
            article.pricing-column.col-xl-4.col-md-6
              .inner-box.card-box
                .plan-header.p-3.text-center
                  h3.plan-title Pro
                  h2.plan-price.font-weight-normal
                    | ksh 11,8000
                  .plan-duration Per Year
                ul.plan-stats.list-unstyled.text-center.p-3.mb-0
                  li 40GB Disk space
                  li unlimited Bandwidth
                  li unlimited Databases
                  li unlimited Emails
                  li unlimited Subdomains
                  li Softaculous Apps
                  li SEO tools
                  li email and phone support
                  li Visitors metrics
                  li 3 years free SSL
                .text-center
                  a.btn.btn-primary.waves-effect.waves-light(style='background-color: #0043ffde' href='#')
                    | purchase Pro
          // end row
      // end row
      // end row
    // container-fluid
  // content
  // Footer Start
  footer.footer
    .container-fluid
      .row
        .col-md-6
          | 2016 - 2020 &copy; Adminto theme by 
          a(href='') Coderthemes
        .col-md-6
          .text-md-right.footer-links.d-none.d-sm-block
            a(href='javascript:void(0);') About Us
            a(href='javascript:void(0);') Help
            a(href='javascript:void(0);') Contact Us
  // end Footer
