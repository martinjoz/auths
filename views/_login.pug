                svg.bi.bi-shield-lock-fill(xmlns='http://www.w3.org/2000/svg' width='26' height='26' fill='blue' viewbox='0 0 16 16')
                  path(fill-rule='evenodd' d='M8 0c-.69 0-1.843.265-2.928.56-1.11.3-2.229.655-2.887.87a1.54 1.54 0 0 0-1.044 1.262c-.596 4.477.787 7.795 2.465 9.99a11.777 11.777 0 0 0 2.517 2.453c.386.273.744.482 1.048.625.28.132.581.24.829.24s.548-.108.829-.24a7.159 7.159 0 0 0 1.048-.625 11.775 11.775 0 0 0 2.517-2.453c1.678-2.195 3.061-5.513 2.465-9.99a1.541 1.541 0 0 0-1.044-1.263 62.467 62.467 0 0 0-2.887-.87C9.843.266 8.69 0 8 0zm0 5a1.5 1.5 0 0 1 .5 2.915l.385 1.99a.5.5 0 0 1-.491.595h-.788a.5.5 0 0 1-.49-.595l.384-1.99A1.5 1.5 0 0 1 8 5z')
              button.close(type='button' data-dismiss='modal' aria-hidden='true') &times;
            .modal-body
              div
                .text-center.mb-4
                  h4.text-uppercase.mt-0.mb-2 Reset Your Password
                  p.mb-0.font-16
                    | Enter your email address and we&apos;ll send you an email with instructions to reset your password.
                form(action='#')
                  .form-group.mb-2
                    label(for='emailaddress') Email address
                    input#emailaddress.form-control(type='email' required='' placeholder='Enter your email')
                  .form-group.mb-0.text-center
                    button.btn.btn-primary.btn-block(style='background-color: blue' type='submit') Reset Password
              // end card-body
          // /.modal-content
        // /.modal-dialog
      // /.modal
      // end col
    // end row
  // end container
// end page
