.navbar-custom
  ul.list-unstyled.topnav-menu.float-right.mb-0
    li.d-none.d-sm-block
      form.app-search
        .app-search-box
          .input-group
            input#emailaddress.form-control(type='text' required='' placeholder='web/app/domain')
            .input-group-append
              button.btn(type='submit')
                i.fe-search
    li.dropdown.notification-list
      a.nav-link.dropdown-toggle.waves-effect(data-toggle='dropdown' href='#' role='button' aria-haspopup='false' aria-expanded='false')
        i.fe-bell.noti-icon
        span.badge.badge-danger.rounded-circle.noti-icon-badge 9
      .dropdown-menu.dropdown-menu-right.dropdown-lg
        // item
        .dropdown-item.noti-title
          h5.m-0
            span.float-right
              a.text-dark(href='')
                small Clear All
            | Notification
        .slimscroll.noti-scroll
          // item
          a.dropdown-item.notify-item(href='javascript:void(0);')
            .notify-icon.bg-primary
              i.mdi.mdi-comment-account-outline
            p.notify-details
              | Caleb Flakelar commented on Admin
              small.text-muted 1 min ago
          // All
          a.dropdown-item.text-center.text-primary.notify-item.notify-all(href='javascript:void(0);')
            | View all
            i.fi-arrow-right
    li.dropdown.notification-list
      a.nav-link.dropdown-toggle.nav-user.mr-0.waves-effect(data-toggle='dropdown' href='#' role='button' aria-haspopup='false' aria-expanded='false')
      if flag=='git_auth'
        img.rounded-circle.img-thumbnail.avatar-md(src=`${userGit.avatar_url}` alt='user-image')
        span.pro-user-name.ml-1
          | #{userGit.login} 
      if flag=='google_auth'
        img.rounded-circle.img-thumbnail.avatar-md(src=`${user.picture}` alt='user-image')
        span.pro-user-name.ml-1
          | #{user.given_name}
      if flag=='fb_auth'
        img.rounded-circle.img-thumbnail.avatar-md(src=`${picture}` alt='user-image')
        span.pro-user-name.ml-1
          |   
          i.mdi.mdi-chevron-down
      .dropdown-menu.dropdown-menu-right.profile-dropdown
        // item
        .dropdown-header.noti-title
          h6.text-overflow.m-0 Welcome.
        // item
        a.dropdown-item.notify-item(href='javascript:void(0);')
          i.fe-user
          span My Account
        .dropdown-divider
        // item
        a.dropdown-item.notify-item(href='/logout')
          i.fe-log-out
          span Logout
  // LOGO
  ul.list-unstyled.topnav-menu.topnav-menu-left.mb-0
    li
      button.button-menu-mobile.disable-btn.waves-effect
        i.fe-menu
    li
      h4.page-title-main
        img(style='height: 50px;' src='/assets/images/trasparent black.png' alt='T' srcset='')
        |               Client Dashboard
